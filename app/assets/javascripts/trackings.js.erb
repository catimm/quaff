$(function() {
	$('#location_tracking_all_seattle').on("change", function(){
		if ($("#new_location_tracking input:checkbox:checked").length > 0) {
			$(".tracking-option").children("label").children("input").removeAttr('checked');
		} 
	});
	$(".tracking-option").children("label").children("input").on("change", function(){
		if ($('#location_tracking_all_seattle').length > 0) {
			$('#location_tracking_all_seattle').removeAttr('checked');
		}
	});
});