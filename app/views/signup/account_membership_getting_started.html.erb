<%= render 'layouts/header_early_signup' %> 
	<div class="row no-margin spinner-signup text-center">
		<div class="spinner-text">
			<i class="fa fa-spinner fa-spin fa-4x" aria-hidden="true"></i>
			<h4>Processing...</h4>
		</div>
	</div>
	<div class="row no-margin first-row-signup">
		<%= render 'signup/signup_guide_consumer_new' %>
	</div>
	<% if session[:new_membership] == "zero" %>
		<%= render 'signup/membership_view_zero' %>
	<% elsif session[:new_membership] == "six" %>
		<%= render 'signup/membership_view_six' %>
	<% else %>
		<%= render 'signup/membership_view_twenty_five' %>
	<% end %>
<%= render 'layouts/footer' %>