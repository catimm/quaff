<% if @customer_next_delivery.status == "admin prep" %>
	<%= render 'admin_delivery_note_button' %>
	<%= render 'admin_user_delivery_button' %>
<% elsif @customer_next_delivery.status == "user review" %>
	<%= render 'admin_user_feedback_button' %>
	<%= render 'admin_review_delivery_button' %>
	<%= render 'admin_delivery_note_button' %>	
<% else %>
	<%= render 'admin_user_feedback_button' %>
	<%= render 'admin_review_delivery_button' %>
<% end %>